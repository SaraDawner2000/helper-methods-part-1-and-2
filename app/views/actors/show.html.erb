<%# render @actor %> <!--what does this do?-->
<div>
  <dl>
    <dt>Name</dt>
    <dd><%= @actor.name %></dd>
    <dt>DOB</dt>
    <dd><%= @actor.dob %></dd>
    <dt>Bio</dt>
    <dd><%= @actor.bio %></dd>
  </dl>
</div>
<hr>
<hr>
<div>
  <%= link_to "Edit this actor", edit_actor_path(@actor) %> |
  <%= link_to "Back to actors", actors_path %>

  <%= button_to "Destroy this actor", @actor, method: :delete %>
</div>
